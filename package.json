{
  "$schema": "http://json.schemastore.org/package",
  "name": "wastelib",
  "version": "0.0.2",
  "description": "JavaScript library (Written in TypeScript) for reading the assets of the game Wasteland.",
  "keywords": [
    "Wasteland",
    "ALLHTDS1",
    "ALLHTDS2",
    "ALLPICS1",
    "ALLPICS2",
    "COLORF.FNT",
    "CURS",
    "END.CPA",
    "GAME1",
    "GAME2",
    "IC0_9.WLF",
    "MASKS.WLF",
    "TITLE.PIC"
  ],
  "homepage": "https://github.com/kayahr/wastelib",
  "author": {
    "name": "Klaus Reimer",
    "email": "k@ailis.de",
    "url": "https://github.com/kayahr/"
  },
  "license": "MIT",
  "main": "lib/main/wastelib",
  "typings": "lib/main/wastelib",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayahr/wastelib.git"
  },
  "scripts": {
    "clean": "rimraf lib doc",
    "compile": "tsc",
    "watch": "tsc -w",
    "doc": "typedoc --excludeNotExported --excludePrivate --mode file --out doc/api/ src/main node_modules/@types/",
    "dist": "webpack",
    "lint": "tslint --project .",
    "prepare": "npm run clean && npm run compile && npm run dist",
    "test": "npm run lint"
  },
  "files": [
    "lib/main",
    "src/main"
  ],
  "directories": {
    "lib": "./lib/"
  },
  "devDependencies": {
    "@types/node": "=12.0.12",
    "@types/source-map-support": "=0.5.0",
    "rimraf": "=2.6.3",
    "source-map-loader": "=0.2.4",
    "steal": "=2.2.1",
    "tslint": "=5.18.0",
    "typedoc": "=0.14.2",
    "typescript": "=3.5.2",
    "webpack": "=4.35.2",
    "webpack-cli": "=3.3.5"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
