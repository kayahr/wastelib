{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.2",
    "files": [
        "**/*"
    ],
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "ignorePaths": [
        "node_modules",
        "coverage",
        "src/sandbox/**",
        "package-lock.json",
        "git",
        "lib",
        "dist",
        "*.log",
        "*.tgz"
    ],
    "words": [
        "ailis",
        "ALLHTDS",
        "allpics",
        "COLORF",
        "devtool",
        "Inplace",
        "kayahr",
        "readwrite",
        "Reimer",
        "savegame",
        "Tileset",
        "Tilesets",
        "tslib",
        "typedoc",
        "Vxor",
        "wastelib"
    ],
    "language": "en",
    "enableFiletypes": [
        "html",
        "json"
    ]
}
